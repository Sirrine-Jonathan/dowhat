{
  "name": "dowhat",
  "version": "1.0.13",
  "description": "CLI to print out readme and available npm scripts in the package",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sirrine-Jonathan/dowhat.git"
  },
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "deploy": "npm version patch && npm publish --access public",
    "postinstall": "node scripts/postinstall.js",
    "postuninstall": "node scripts/postuninstall.js"
  },
  "keywords": [
    "cli",
    "readme",
    "scripts"
  ],
  "author": "sirrineprogramming@gmail.com",
  "license": "ISC",
  "bin": {
    "dowhat": "bin/cli.js"
  },
  "directories": {
    "bin": "bin",
    "man": "man"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "expand-tilde": "^2.0.2",
    "figlet": "^1.8.0",
    "inquirer": "^12.4.1",
    "marked": "^14.1.4",
    "marked-terminal": "^7.2.1"
  }
}
